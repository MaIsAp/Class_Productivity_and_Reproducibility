@article{Navarro2017Monitoring,
  title = {Monitoring Biodiversity Change through Effective Global Coordination},
  author = {Navarro, Laetitia M and Fern{\'a}ndez, N{\'e}stor and Guerra, Carlos and Guralnick, Rob and Kissling, W Daniel and Londo{\~n}o, Maria Cecilia and {Muller-Karger}, Frank and Turak, Eren and Balvanera, Patricia and Costello, Mark J and Delavaud, Aurelie and El Serafy, {\relax GY} and Ferrier, Simon and Geijzendorffer, Ilse and Geller, Gary N and Jetz, Walter and Kim, Eun-Shik and Kim, HyeJin and Martin, Corinne S and McGeoch, Melodie A and Mwampamba, Tuyeni H and Nel, Jeanne L and Nicholson, Emily and Pettorelli, Nathalie and Schaepman, Michael E and Skidmore, Andrew and Sousa Pinto, Isabel and Vergara, Sheila and Vihervaara, Petteri and Xu, Haigen and Yahara, Tetsukazu and Gill, Mike and Pereira, Henrique M},
  year = {2017},
  month = dec,
  journal = {Current Opinion in Environmental Sustainability},
  volume = {29},
  pages = {158--169},
  issn = {1877-3435},
  doi = {10.1016/j.cosust.2018.02.005},
  urldate = {2022-02-03},
  abstract = {The ability to monitor changes in biodiversity, and their societal impact, is critical to conserving species and managing ecosystems. While emerging technologies increase the breadth and reach of data acquisition, monitoring efforts are still spatially and temporally fragmented, and taxonomically biased. Appropriate long-term information remains therefore limited. The Group on Earth Observations Biodiversity Observation Network (GEO BON) aims to provide a general framework for biodiversity monitoring to support decision-makers. Here, we discuss the coordinated observing system adopted by GEO BON, and review challenges and advances in its implementation, focusing on two interconnected core components\textemdash the Essential Biodiversity Variables as a standard framework for biodiversity monitoring, and the Biodiversity Observation Networks that support harmonized observation systems\textemdash while highlighting their societal relevance.},
  langid = {english},
  file = {/home/miap/Zotero/storage/AENZ4U6K/Navarro et al_2017_Monitoring biodiversity change through effective global coordination.pdf;/home/miap/Zotero/storage/R8A636H8/Navarro et al. - 2017 - Monitoring biodiversity change through effective g.pdf;/home/miap/Zotero/storage/VC6898S8/S1877343517301665.html}
}

@article{Nichols2006Monitoring,
  title = {Monitoring for Conservation},
  author = {Nichols, James D. and Williams, Byron K.},
  year = {2006},
  month = dec,
  journal = {Trends in Ecology \& Evolution},
  volume = {21},
  number = {12},
  pages = {668--673},
  issn = {0169-5347},
  doi = {10.1016/j.tree.2006.08.007},
  urldate = {2021-02-26},
  abstract = {Human-mediated environmental changes have resulted in appropriate concern for the conservation of ecological systems and have led to the development of many ecological monitoring programs worldwide. Many programs that are identified with the purpose of `surveillance' represent an inefficient use of conservation funds and effort. Here, we revisit the 1964 paper by Platt and argue that his recommendations about the conduct of science are equally relevant to the conduct of ecological monitoring programs. In particular, we argue that monitoring should not be viewed as a stand-alone activity, but instead as a component of a larger process of either conservation-oriented science or management. Corresponding changes in monitoring focus and design would lead to substantial increases in the efficiency and usefulness of monitoring results in conservation.},
  langid = {english},
  file = {/home/miap/Zotero/storage/GX2UPS9A/S0169534706002680.html}
}

